<h3>Genre Details</h3><br></br>
<p>Genre Name:<%= @genre.name %></p>

<p><strong>Genre Songs:</strong><% @genre.songs.each do |song| %></p>
<p>Song Name: <a href="/songs/<%= song.slug %>"> <%= song.name %> </a></p>
<% end %>
<p><strong>Genre Artists:</strong><% @genre.artists.each do |artist| %></p>
<p>Artist Name: <a href="/artists/<%= artist.slug %>"><%= artist.name %></a></p>
<% end %>
